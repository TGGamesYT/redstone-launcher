{
  "name": "redstone-launcher",
  "productName": "Redstone Launcher",
  "version": "1.13.2",
  "description": "A Minecraft launcher",
  "author": "Redstone Launcher Team",
  "main": "main.js",
  "scripts": {
    "app": "electron .",
    "buildWin": "electron-builder --win --publish=never",
    "buildLinux": "electron-builder --linux AppImage --publish=never",
    "buildMac": "electron-builder --mac --publish=never"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "base64url": "^3.0.1",
    "child_process": "^1.0.2",
    "crypto": "^1.0.1",
    "discord-rpc": "^4.0.1",
    "electron-store": "^10.1.0",
    "fernet": "^0.3.3",
    "form-data": "^4.0.4",
    "http": "^0.0.1-security",
    "minecraft-launcher-core": "^3.18.2",
    "msmc": "^5.0.5",
    "namemc-skinhistory": "^1.0.1",
    "node-fetch": "^3.3.2",
    "prismarine-nbt": "^2.7.0",
    "skinview3d": "^3.4.1",
    "tar": "^7.5.2",
    "tar-fs": "^3.1.1",
    "three": "^0.180.0",
    "tomate-loaders": "^2.0.2",
    "unzipper": "^0.12.3"
  },
  "build": {
    "appId": "com.redstone-launcher.app",
    "productName": "Redstone Launcher",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "frontend/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "frontend/icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "frontend/icon.icns"
    }
  }
}
